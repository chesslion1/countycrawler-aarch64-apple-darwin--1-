{
  "name": "county-crawler-playwright",
  "version": "0.4.9",
  "type": "module",
  "private": true,
  "scripts": {
    "postinstall": "npx playwright install --with-deps chromium",
    "build": "tsc -p tsconfig.json",
    "crawl:adams": "node dist/index.js --county adams",
    "crawl": "npm run build -- --pretty && npm run crawl:adams"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "date-fns": "^4.1.0",
    "playwright": "^1.48.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
